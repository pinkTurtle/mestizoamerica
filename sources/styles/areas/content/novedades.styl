.content
  padding-top: 20px
  .icon-tips
    &:before
      color: $dark-gray
  .principal-content
    width: 65%
    float: left
    margin-top: 20px
    padding-right: 15px
    .article
      margin-top: 0
      padding-bottom: 30px
  aside 
    width: 35%
    float: right
    margin-top: 20px
    .box-title  
      background-color: $light-blue
      height: 40px
      .title-links 
        a 
          color: white
          font-family: $hightlight
          font-size: 15px
          padding: 0
          margin: 0
          line-height: 40px
          padding-left: 20px
      a
        color: $gray
        font-size: 12px
    .box-links   
      background-color: $Xlight-gray
      padding-bottom: 10px
      ul
        li
          font-size: 12px
          color: $gray
          padding-left: 20px
          p 
            text-transform: uppercase
            line-height: 15px
            padding-top: 15px
            font-weight: bold
          a
            color: $gray
            